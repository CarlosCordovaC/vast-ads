<VAST xmlns="http://www.iab.com/VAST" version="3.0">
  <Ad id="sample-ad">
    <InLine>
      <AdSystem>Brightcove Ad System</AdSystem>
      <AdTitle>Sample Non-Linear Ad</AdTitle>
      <Description>Ad with hosted MP4 video on Dropbox</Description>
      <Creatives>
        <Creative>
          <NonLinearAds>
            <NonLinear width="300" height="100" scalable="true" maintainAspectRatio="true">
              <StaticResource creativeType="image/png">
                <![CDATA[ https://www.example.com/nonlinear-ad-banner.png ]]>
              </StaticResource>
              <NonLinearClickThrough>
                <![CDATA[ https://example.com/click-through-url ]]>
              </NonLinearClickThrough>
            </NonLinear>
          </NonLinearAds>
        </Creative>
      </Creatives>
    </InLine>
  </Ad>
</VAST>
